help		!	.	HELP		0
dryrun		!	.	DRYRUN		0
author		=s	.	AUTHOR		'matevz.tadel@cern.ch'

gledver		=s	.	GLED_VERSION	'1.3.0'
cvstag		=s	.	GLED_CVS_TAG	'HEAD'
#cvstag		=s	.	GLED_CVS_TAG	'v1-3-0'
echo-config	!	.	ECHO_CONFIG	0

config		=s	.	CONFIG		"build-config"
buildname	=s	.	BUILDNAME	"x86_64-gentoo-gcc4.2.3"
# buildname	=s	.	BUILDNAME	0
sysname		=s	.	SYSNAME		"linux"

external	=s	.	EXTERNAL	0

skip-external	!	.	SKIP_EXTERNAL	0
skip-libset	!	.	SKIP_LIBSET	0
skip-meta	!	.	SKIP_META	1
meta-pack-srcs	!	.	META_PACK_SRCS	0

keep-external	!	.	KEEP_EXTERNAL	0
keep-libset	!	.	KEEP_LIBSET	0
keep-meta	!	.	KEEP_META	0

# Do not remove build/install directories on start-up.
noremove	!	.	NOREMOVE	0
